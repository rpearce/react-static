{
  "name": "statik-js",
  "version": "0.1.0",
  "description": "Static site generator for Node.js",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -r _site && mkdir _site",
    "build:js": "browserify src/browser.js -o _site/app.js",
    "build:static": "babel-node src/index.js",
    "build": "npm run clean && npm run build:static && npm run build:js",
    "prebuild": "npm run lint"
  },
  "keywords": [
    "static",
    "generator",
    "static",
    "node",
    "nodejs"
  ],
  "author": "Robert Pearce",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.4.0",
    "browserify": "^12.0.0",
    "eslint": "^1.6.0"
  },
  "dependencies": {
    "fs-promise": "^0.3.1",
    "history": "^1.12.6",
    "js-yaml": "^3.4.2",
    "marked": "^0.3.5",
    "parse-md": "^0.1.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "optional": [
            "es7.asyncFunctions",
            "es7.exportExtensions"
          ]
        }
      ]
    ]
  },
  "private": true
}
