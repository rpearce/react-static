{
  "name": "react-static",
  "version": "0.1.0",
  "description": "React static site generator framework for Node.js",
  "main": "index.js",
  "scripts": {
    "clean": "rm -f index.js",
    "lint": "eslint src",
    "watch": "babel lib/react-static.js -w -o index.js",
    "build": "babel lib/react-static.js -o index.js",
    "prebuild": "npm run clean && npm run lint"
  },
  "keywords": [
    "react",
    "reactjs",
    "static",
    "static site",
    "site generator",
    "static site framework",
    "react static",
    "react static framework"
  ],
  "author": "Robert Pearce",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.29",
    "commander": "^2.9.0",
    "nodemon": "^1.8.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.4",
    "eslint": "^1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpearce/react-static.git"
  },
  "bugs": {
    "url": "https://github.com/rpearce/react-static/issues"
  },
  "homepage": "https://github.com/rpearce/react-static#readme",
  "preferGlobal": "true",
  "bin": {
    "react-static": "index.js"
  }
}
