{
  "name": "react-static",
  "version": "0.1.0",
  "description": "React static site generator for Node.js",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf example/_site",
    "lint": "eslint src",
    "build:example": "npm run clean && npm run build && babel-node index.js",
    "build": "babel lib/react-static.js -o lib/index.js",
    "prebuild": "npm run lint"
  },
  "keywords": [
    "react",
    "reactjs",
    "static",
    "static site",
    "site generator"
  ],
  "author": "Robert Pearce",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.4.0",
    "browserify": "^12.0.1",
    "eslint": "^1.6.0",
    "fs-promise": "^0.3.1",
    "mkdirp": "^0.5.1",
    "uglify-js": "^2.5.0"
  },
  "dependencies": {
    "history": "^1.12.6",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  }
}
